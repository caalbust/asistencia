<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ControlacFIEC\TodoBundle\Entity\Rol" table="rol">
    <id name="rolId" type="integer" column="rol_id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="rol_rol_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="rolName" type="string" column="rol_name" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
