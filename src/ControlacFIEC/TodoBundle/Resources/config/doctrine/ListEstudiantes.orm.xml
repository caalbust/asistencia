    <many-to-one field="listEstudiantesUsuario" target-entity="Estudiantes" fetch="LAZY">
      <join-columns>
        <join-column name="list_estudiantes_usuario" referenced-column-name="est_id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
