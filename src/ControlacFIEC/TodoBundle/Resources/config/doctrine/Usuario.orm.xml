<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="ControlacFIEC\TodoBundle\Entity\Usuario" table="usuario">
    <id name="userId" type="integer" column="user_id">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="usuario_user_id_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="usuarioName" type="string" column="usuario_name" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usuarioApellido" type="string" column="usuario__apellido" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usuarioMail" type="string" column="usuario_mail" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usuarioUser" type="string" column="usuario_user" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="usuarioPasword" type="string" column="usuario_pasword" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
